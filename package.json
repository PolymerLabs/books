{
  "name": "books",
  "version": "0.1.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "PolymerLabs/books",
  "scripts": {
    "start": "polymer serve",
    "build": "npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "deploy": "gcloud app deploy server/app.yaml --project books-pwakit",
    "test": "npm run test:unit",
    "test:unit": "polymer test --module-resolution=node --npm"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0-pre.19",
    "@polymer/lit-element": "^0.5.2",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "pwa-helpers": "^0.8.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.2.3",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.1.0",
    "polymer-cli": "^1.7.2",
    "prpl-server": "^1.1.0",
    "wct-browser-legacy": "^1.0.1"
  }
}
