{
  "name": "books",
  "version": "0.1.0",
  "flat": "true",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "PolymerLabs/books",
  "scripts": {
    "start": "polymer serve",
    "build": "npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && rm -rf server/build && mv build server/ && renamer --find node_modules --replace node_assets 'server/build/**' && replace node_modules node_assets server/build -r -s",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es6-bundled",
    "deploy": "gcloud app deploy server/app.yaml --project books-pwakit",
    "test": "npm run test:unit",
    "test:unit": "polymer test --module-resolution=node --npm -l chrome -l firefox"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0-pre.15",
    "@polymer/lit-element": "^0.4.0",
    "@webcomponents/webcomponentsjs": "^2.0.0-beta.2",
    "pwa-helpers": "polymerlabs/pwa-helpers",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "polymer-cli": "^1.7.0-pre.17",
    "prpl-server": "^1.1.0",
    "renamer": "^0.6.1",
    "replace": "^0.3.0",
    "wct-browser-legacy": "0.0.1-pre.11"
  }
}
